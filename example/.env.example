# Example Environment Configuration
# Copy this file to .env, .env.dev, .env.prod etc. and update with your actual values
# DO NOT commit actual .env files to version control!

# API Configuration
API_URL=https://your-api-url-here.com
GRAPHQL_ENDPOINT=https://your-graphql-endpoint.com/

# WebSocket Configuration  
SOCKET_URL=wss://your-websocket-url.com/

# Debug Settings
DEBUG=true
LOG_LEVEL=verbose
ENABLE_MOCK_DATA=false

# Timeouts (in milliseconds)
API_TIMEOUT=30000
SOCKET_TIMEOUT=5000

# Feature Flags
ENABLE_CRASHLYTICS=false
ENABLE_ANALYTICS=false
ENABLE_PERFORMANCE_MONITORING=true

# Add your custom variables below
# CUSTOM_VAR=value